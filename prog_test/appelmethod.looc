class Item = (
  var current : int;
  var previous : int;
  var generation : int;

  method init(a : int, b : string )
  {
    current := 1;
    previous := 0;
    generation := 1;
  }

  method nextGen(x : int, y : int)
  {
    var temp : int;
    temp := current;
    current := current + previous;
    previous := temp;
    generation := generation + 1;
  }

  method getNb() : int {
    return (current);
  }
)

